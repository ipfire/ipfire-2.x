+		}
+		capi_handle_msg(monCMSG);
+    		break;
+	    case 0x1104:
+		// CAPI queue is empty
+    		break;
+	    default:
+		// something is wrong!
+		 break;
+	} //switch
+	free(monCMSG);
+    } // for
+    // never reached
+    return NULL;
