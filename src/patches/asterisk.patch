+static void *do_monitor(void *data) {
+   unsigned int Info;
+    _cmsg *monCMSG;
+    for (;;) {
+/*
+	if (ast_mutex_lock(&monlock)) {
+	    ast_log(LOG_ERROR,"Unable to get monitor lock!\n");
