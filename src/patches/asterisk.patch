+#ifdef CAPI_GAIN
+static void capi_gains(struct ast_capi_gains *g,float rxgain,float txgain) {
+    int i=0;
+    int x=0;
+    if (rxgain != 1.0) {
+	for (i=0;i<256;i++) {
+	    x = (int)(((float)capiXLAW2INT(i)) * rxgain);
+	    if (x > 32767) x = 32767;
+	    if (x < -32767) x = -32767;
+	    g->rxgains[i] = capiINT2XLAW(x);
+	}
+    } else {
+	for (i=0;i<256;i++) {
+	    g->rxgains[i] = i;
+	}
+    }
+    if (txgain != 1.0) {
+	for (i=0;i<256;i++) {
+	    x = (int)(((float)capiXLAW2INT(i)) * txgain);
+	    if (x > 32767) x = 32767;
+	    if (x < -32767) x = -32767;
+	    g->txgains[i] = capiINT2XLAW(x);
+	}
+    } else {
+	for (i=0;i<256;i++) {
+	    g->txgains[i] = i;
+	}
+    }
+    
+}
