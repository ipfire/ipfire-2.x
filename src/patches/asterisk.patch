+	// do some nifty stuff
+	ast_mutex_unlock(&monlock);
+*/
+	monCMSG = malloc(sizeof(_cmsg));
+	memset(monCMSG,0,sizeof(_cmsg));
+	switch(Info = check_wait_get_cmsg(monCMSG)) {
+	    case 0x0000:
+		if (option_verbose > 8) {
+    		    if (capidebug)
