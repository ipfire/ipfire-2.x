From 06c8bdf0f3625408906cba90f05dfa4980c6b960 Mon Sep 17 00:00:00 2001
From: Arne Fitzenreiter <arne_f@ipfire.org>
Date: Mon, 8 Dec 2025 14:48:02 +0100
Subject: [PATCH 8/8] rockchip: dt: nanopi-r4s: add realtek pcie devicenode

Signed-off-by: Arne Fitzenreiter <arne_f@ipfire.org>
---
 .../boot/dts/rockchip/rk3399-nanopi-r4s.dtsi      | 15 +++++++++++++++
 1 file changed, 15 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3399-nanopi-r4s.dtsi b/arch/arm64/boot/dts/rockchip/rk3399-nanopi-r4s.dtsi
index 24e8a17da49e..189baefa8d4a 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399-nanopi-r4s.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3399-nanopi-r4s.dtsi
@@ -17,6 +17,10 @@
 #include "rk3399-nanopi4.dtsi"
 
 / {
+	aliasses {
+		ethernet1 = &r8169;
+	};
+
 	/delete-node/ display-subsystem;
 
 	gpio-leds {
@@ -75,6 +79,17 @@ &pcie0 {
 	max-link-speed = <1>;
 	num-lanes = <1>;
 	vpcie3v3-supply = <&vcc3v3_sys>;
+
+	pcie@0 {
+		reg = <0x00000000 0 0 0 0>;
+		#address-cells = <3>;
+		#size-cells = <2>;
+
+		r8169: pcie@0,0 {
+			reg = <0x00000000 0 0 0 0>;
+			local-mac-address = [ 00 00 00 00 00 00 ];
+		};
+	};
 };
 
 &pinctrl {
